*
IF $OCCURS(CLE_DOSSIER_CONNEXE) NE $OCCURS(NUM_PIECE_CONNEXE) THEN;
  CHECK NUM_PIECE_CONNEXE, ALWAYS_FAILS, MESSAGE='Clés dossier et Nos piéce +
 connexes doivent étre appairés!';
END_IF;
END_REVL;


